   "source": [
    "#1463 1로 만들기\n",
    "N = int(input())\n",
    "a = [0] * (N+1) # a[0]은 1을 만드는 수\n",
    "for i in range(2,N+1):\n",
    "    a[i] = a[i-1] + 1\n",
    "    if i % 2 == 0:\n",
    "        a[i] = min(a[i//2]+1,a[i])\n",
    "    if i % 3 == 0:\n",
    "        a[i] = min(a[i//3]+1,a[i])\n",
    "print(a[N])\n"
   ]
